{
  "private": true,
  "name": "react-intro-hooks",
  "version": "2.3.2",
  "scripts": {
    "start": "mdx-deck -p 1337 deck.mdx",
    "build": "mdx-deck build deck.mdx",
    "help": "mdx-deck",
    "gh-pages": "git subtree push --prefix dist origin gh-pages",
    "gh-pages:force": "git push origin `git subtree split --prefix dist master`:gh-pages --force",
    "gh-pages:refresh": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "deploy": "npm run build && cd dist && npx now",
    "alias": "npx now alias https://dist-tcuzlrgcyt.now.sh react-intro-hooks"
  },
  "now": {
    "alias": "react-intro-hooks"
  },
  "devDependencies": {
    "file-loader": "^4.0.0",
    "mdx-deck": "^2.3.2",
    "mdx-deck-live-code": "^1.0.1"
  },
  "keywords": [],
  "description": "Deck created with mdx-deck",
  "dependencies": {
    "code-surfer": "^2.0.0-alpha.4",
    "polished": "^3.4.0",
    "prism-react-renderer": "^0.1.6",
    "raw-loader": "^2.0.0",
    "react-hook-clipboard": "^1.0.0",
    "react-hotkeys-hook": "^1.4.0",
    "react-live": "^2.1.2",
    "styled-components": "^4.3.1",
    "uifx": "^1.0.7"
  }
}
